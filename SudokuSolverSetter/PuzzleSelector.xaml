        <Expander Header="Expander" HorizontalAlignment="Left" Height="100" Margin="10,170,0,0" VerticalAlignment="Top" Width="170">
            <Grid Background="#FFE5E5E5"/>
        </Expander>

    </Grid>
</Window>
